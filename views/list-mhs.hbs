<div class="right_col" role="main">
          <div class="">
            <div class="page-title">
              <div class="title_left">
                <h3>List Mahasiswa</h3>
              </div>
              <div class="title_right">
                <div class="col-md-5 col-sm-5 col-xs-12 form-group pull-right top_search">
                  <div class="input-group">
                    <input type="text" class="form-control" placeholder="Search for...">
                    <span class="input-group-btn">
                      <button class="btn btn-default" type="button">Go!</button>
                    </span>
                  </div>
                </div>
              </div>
            </div>
            <a class="btn btn-raised btn-info" href="/add-mhs">Tambah Mahasiswa</a>
            <div class="clearfix"></div>
            <div class="row">
              <div class="col-md-11 col-sm-11 col-xs-11">
                <div class="x_panel">
                  <div class="x_title">
                    <h2>Informasi Mahasiswa</h2>
                    <ul class="nav navbar-right panel_toolbox">
                      <li><a class="collapse-link"><i class="fa fa-chevron-up"></i></a>
                      </li>
                      <li class="dropdown">
                        <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-expanded="false"><i class="fa fa-wrench"></i></a>
                        <ul class="dropdown-menu" role="menu">
                          <li><a href="#">Settings 1</a>
                          </li>
                          <li><a href="#">Settings 2</a>
                          </li>
                        </ul>
                      </li>
                      <li><a class="close-link"><i class="fa fa-close"></i></a>
                      </li>
                    </ul>
                    <div class="clearfix"></div>
                  </div>
                  <div class="x_content">                 
                  <table class="table table-hover">
                      <thead>
                        <tr>
                          <th>#</th>
                          <th>Nama</th>
                          <th>Kelas</th>
                          <th>NPM</th>
                          <th>Email</th>
                          <th>Action</th>
                        </tr>
                      </thead>
                      <tbody>
                  {{#each getData.data}}
                    <tr>
                        <td>
                          {{@index}}
                        </td>
                        <td>
                            {{this.nama}}
                        </td>
                         <td>
                            {{this.kelas}}
                        </td>
                        
                        <td>
                            {{this.npm}}
                        </td>

                        <td>
                            {{this.email}}
                        </td>                 



                        <td id="btn-edit" class="tengah text-center"> <a class="btn btn-raised btn-info" href="/update-mhs/{{this.npm}}">Update</a></td>
                        <td class="tengah text-center"> <a  id="btn-delete" onclick="deleteById({{this.npm}})" class="btn btn-raised btn-info">Delete</a></td>
                        {{/each}}
                      </tbody>
                    </table>
                  </div>
                </div>
              </div>







            

   
            </div>
          </div>
        </div>


